{
	"name": "unit-test-generator",
	"displayName": "unit-test-generator",
	"description": "Automatic unit test code genrator.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:extension.generateUnitTest",
		"workspaceContains:**/.unittestgeneratorconfig",
		"onLanguage:typescript"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World",
				"category": "Extension Listener"
			},
			{
				"command": "extension.generateUnitTest",
				"title": "Generate Unit Test",
				"category": "Extension Listener"
			}
		],
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
