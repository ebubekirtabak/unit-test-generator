{
	"name": "unit-test-generator",
	"displayName": "unit-test-generator",
	"description": "Automatic unit test code genrator.",
	"publisher": "ebubekirtabak",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.generateUnitTest",
		"workspaceContains:**/.unittestgeneratorconfig",
		"onLanguage:typescript",
		"onLanguage:javascript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.generateUnitTest",
				"title": "Generate Unit Test",
				"category": "Extension Listener"
			},
			{
				"command": "extension.addFunction",
				"title": "Add Console Log"
			}
		],
		"keybindings": [
			{
			  "command": "extension.generateUnitTest",
			  "key": "ctrl+alt+u",
			  "mac": "cmd+alt+u",
			  "when": "editorTextFocus"
			}
		  ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
